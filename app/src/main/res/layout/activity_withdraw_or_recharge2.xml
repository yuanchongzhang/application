<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    android:orientation="vertical"
    tools:context=".activity.WithdrawOrRechargeActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="100px"
        android:background="@color/colorMainBlue"
        android:tag="(0,100)">

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:tag="(0,0,32)"
            android:text="@string/wor_title_01"
            android:textColor="@color/colorWhite"
            tools:textSize="32px" />

        <ImageView
            android:id="@+id/iv_back"
            android:layout_width="64px"
            android:layout_height="78px"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_centerInParent="true"
            android:contentDescription="@string/app_name"
            android:onClick="onBack"
            android:src="@mipmap/ic_action_back"
            android:tag="[20,20,20,20](64,78)"
            android:visibility="visible"
            tools:padding="20px" />

        <TextView
            android:id="@+id/tv_title_right"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentRight="true"
            android:layout_marginRight="25px"
            android:layout_marginTop="34px"
            android:tag="{0,34,25,0}(0,0,28)"
            android:text="@string/wor_title_right_01"
            android:textColor="@color/colorWhite"
            android:textSize="28px" />

    </RelativeLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/ll_bank"
                android:layout_width="match_parent"
                android:layout_height="146px"
                android:orientation="vertical"
                android:tag="(0,146)"
                android:visibility="gone">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="145px"
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:tag="(0,145)">

                    <com.facebook.drawee.view.SimpleDraweeView
                        android:id="@+id/iv_logo"
                        android:layout_width="66px"
                        android:layout_height="66px"
                        android:layout_marginLeft="37px"
                        android:src="@mipmap/logo_boc"
                        android:tag="{37,0,0,0}(66,66)" />

                    <TextView
                        android:id="@+id/tv_name"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="25px"
                        android:layout_weight="1"
                        android:tag="{25,0,0,0}(0,0,26)"
                        android:text="银行名称"
                        android:textColor="@color/colorBlack"
                        android:textSize="26px" />

                    <TextView
                        android:id="@+id/tv_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="20px"
                        android:tag="{0,0,20,0}(0,0,26)"
                        android:text="6000 3000 1254 2541"
                        android:textColor="@color/colorBlack"
                        android:textSize="26px" />

                    <ImageView
                        android:id="@+id/iv_settings_01"
                        android:layout_width="51px"
                        android:layout_height="58px"
                        android:layout_marginRight="20px"
                        android:src="@mipmap/ic_action_getinto_pinkarrow"
                        android:tag="{0,0,20,0}(51,58)" />
                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1px"
                    android:layout_marginLeft="54px"
                    android:background="@color/colorGreydividingline2"
                    android:tag="{54,0,0,0}(0,1)" />
            </LinearLayout>

            <android.support.v7.widget.RecyclerView
                android:id="@+id/rv_01"
                android:layout_width="match_parent"
                android:layout_height="292px"
                android:tag="(0,292)"
                android:visibility="gone"
                tools:listitem="@layout/list_03_item_view">

            </android.support.v7.widget.RecyclerView>

            <LinearLayout
                android:id="@+id/ll_add"
                android:layout_width="match_parent"
                android:layout_height="141px"
                android:clickable="true"
                android:gravity="center"
                android:orientation="horizontal"
                android:tag="(0,141)">

                <ImageView
                    android:layout_width="50px"
                    android:layout_height="50px"
                    android:src="@mipmap/ic_action_add_blue"
                    android:tag="(50,50)" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="20px"
                    android:tag="{20,0,0,0}(0,0,26)"
                    android:text="@string/bm_01"
                    android:textColor="@color/colorMainBlue"
                    tools:textSize="26px" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:background="@color/colorGreydividingline2"
                android:tag="(0,1)"></View>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="97px"
                android:layout_marginTop="38px"
                android:orientation="horizontal"
                android:tag="{0,38,0,0}(0,97)">

                <LinearLayout
                    android:layout_width="57px"
                    android:layout_height="match_parent"
                    android:baselineAligned="false"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:tag="(57,0)">

                    <ImageView
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="5px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,5,1,0}(11,11)"
                        android:visibility="invisible" />

                    <ImageView
                        android:layout_width="19px"
                        android:layout_height="19px"
                        android:layout_marginTop="11px"
                        android:src="@mipmap/ic_symbol_circle_big_red"
                        android:tag="{0,11,0,0}(19,19)"
                        android:visibility="visible" />

                    <ImageView
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="11px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,11,1,0}(11,11)"
                        android:visibility="visible" />

                    <ImageView
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="12px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,12,1,0}(11,11)"
                        android:visibility="visible" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_01"
                    android:layout_width="242px"
                    android:layout_height="wrap_content"
                    android:tag="{3,21,0,0}(242,0,26)"
                    android:text="@string/wor_01"
                    android:textColor="@color/colorBlack"
                    tools:layout_marginLeft="3px"
                    tools:layout_marginTop="20px"
                    tools:textSize="26px" />

                <TextView
                    android:id="@+id/tv_02"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="26px"
                    android:layout_weight="1"
                    android:gravity="right"
                    android:tag="{3,20,26,0}(242,0,26)"
                    android:text="2000.0元"
                    android:textColor="@color/colorBlack"
                    tools:layout_marginLeft="3px"
                    tools:layout_marginTop="20px"
                    tools:textSize="26px" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="97px"
                android:orientation="horizontal"
                android:tag="(0,97)">

                <LinearLayout
                    android:layout_width="57px"
                    android:layout_height="match_parent"
                    android:baselineAligned="false"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:tag="(57,0)">

                    <ImageView
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="5px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,5,1,0}(11,11)"
                        android:visibility="visible" />

                    <ImageView
                        android:layout_width="19px"
                        android:layout_height="19px"
                        android:layout_marginTop="11px"
                        android:src="@mipmap/ic_symbol_circle_big_red"
                        android:tag="{0,11,0,0}(19,19)"
                        android:visibility="visible" />

                    <ImageView
                        android:id="@+id/iv_01"
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="11px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,11,1,0}(11,11)"
                        android:visibility="visible" />

                    <ImageView
                        android:id="@+id/iv_02"
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="12px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,12,1,0}(11,11)"
                        android:visibility="visible" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tv_03"
                    android:layout_width="242px"
                    android:layout_height="wrap_content"
                    android:tag="{3,6,0,0}(242,0,26)"
                    android:text="@string/wor_03"
                    android:textColor="@color/colorBlack"
                    tools:layout_marginLeft="3px"
                    tools:layout_marginTop="10px"
                    tools:textSize="26px" />

                <EditText
                    android:id="@+id/et_01"
                    android:layout_width="394px"
                    android:layout_height="51px"
                    android:background="@drawable/selector_bg_main_04_edit"
                    android:gravity="center"
                    android:hint="@string/wor_03_hint"
                    android:inputType="numberDecimal"
                    android:maxLines="1"
                    android:tag="{0,12,0,0}[5,0,5,0](394,51,26)"
                    tools:layout_marginTop="12px"
                    tools:paddingLeft="5px"
                    tools:paddingRight="5px"
                    tools:textSize="26px" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/ll_01"
                android:layout_width="match_parent"
                android:layout_height="97px"
                android:orientation="horizontal"
                android:tag="(0,97)">

                <LinearLayout
                    android:layout_width="57px"
                    android:layout_height="match_parent"
                    android:baselineAligned="false"
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:tag="(57,0)">

                    <ImageView
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="5px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,5,1,0}(11,11)"
                        android:visibility="visible" />

                    <ImageView
                        android:layout_width="19px"
                        android:layout_height="19px"
                        android:layout_marginTop="11px"
                        android:src="@mipmap/ic_symbol_circle_big_red"
                        android:tag="{0,11,0,0}(19,19)"
                        android:visibility="visible" />

                    <ImageView
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="11px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,11,1,0}(11,11)"
                        android:visibility="gone" />

                    <ImageView
                        android:layout_width="11px"
                        android:layout_height="11px"
                        android:layout_marginTop="12px"
                        android:src="@mipmap/ic_symbol_circle_mid_grey"
                        android:tag="{0,12,1,0}(11,11)"
                        android:visibility="gone" />
                </LinearLayout>

                <TextView
                    android:layout_width="242px"
                    android:layout_height="wrap_content"
                    android:tag="{3,6,0,0}(242,0,26)"
                    android:text="@string/wor_04"
                    android:textColor="@color/colorBlack"
                    tools:layout_marginLeft="3px"
                    tools:layout_marginTop="10px"
                    tools:textSize="26px" />

                <EditText
                    android:id="@+id/et_02"
                    android:layout_width="394px"
                    android:layout_height="51px"
                    android:background="@drawable/selector_bg_main_04_edit"
                    android:gravity="center"
                    android:hint="@string/wor_04_hint"
                    android:inputType="numberPassword"
                    android:maxLines="1"
                    android:tag="{0,12,0,0}[5,0,5,0](394,51,26)"
                    tools:layout_marginTop="12px"
                    tools:paddingLeft="5px"
                    tools:paddingRight="5px"
                    tools:textSize="26px" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"></View>

            <Button
                android:id="@+id/bt_01"
                android:layout_width="match_parent"
                android:layout_height="80px"
                android:background="@drawable/selector_bg_main_04_bt"
                android:tag="(0,80,26)"
                android:text="@string/wor_06"
                android:textColor="@color/colorWhite" />
        </LinearLayout>

        <WebView
            android:id="@+id/wv_01"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none"
            android:visibility="gone"
            android:gravity="center_vertical"
            android:tag="(0,0)"/>
    </FrameLayout>
</LinearLayout>
